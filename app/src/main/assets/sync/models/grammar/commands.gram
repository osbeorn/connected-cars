#JSGF V1.0;

grammar commands;

<takePictureCmd>    = take a picture [ front ];
<connectDeviceCmd>  = connect device;
<setDescriptionCmd> = set message;
<sendCmd>           = send;

public <basicCmd> = <takePictureCmd>    |
                    <connectDeviceCmd>  |
                    <setDescriptionCmd> |
                    <sendCmd>;
