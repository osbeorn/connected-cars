#JSGF V1.0;

grammar digits;

<digit> = (one | first)     |
          (two | second)    |
          (three | third)   |
          (four | fourth)   |
          (five | fifth)    |
          (six | sixth)     |
          (seven | seventh) |
          (eight | eighth)  |
          (nine | ninth)    |
          (ten | tenth)     ;

public <digits> = select <digit>;
